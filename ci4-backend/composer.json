{
  "name": "shakib/auto-printing-system",
  "description": "Remote PDF Printing System with CI4 Backend",
  "license": "MIT",
  "authors": [
    {
      "name": "Shakib Hossain",
      "email": "contact@shakib.me"
    }
  ],
  "require": {
    "php": "^8.0",
    "codeigniter4/framework": "^4.3",
    "endroid/qr-code": "^4.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9",
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit": "^9.1",
    "codeigniter4/codeigniter4-standard": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Config\\": "app/Config/"
    },
    "exclude-from-classmap": [
      "**/Database/Migrations/**"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Support\\": "tests/_support"
    }
  },
  "scripts": {
    "test": "phpunit",
    "post-update-cmd": [
      "CodeIgniter\\ComposerScripts::postUpdate"
    ]
  },
  "support": {
    "forum": "http://forum.codeigniter.com/",
    "source": "https://github.com/codeigniter4/CodeIgniter4",
    "issues": "https://github.com/ProgrammerSMSH/auto-printing-system/issues"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
