# Home Server Configuration
API_BASE_URL=https://your-domain.com/api
API_KEY=your-secure-api-key-here

# Path Configuration
TEMP_DIR=/home/printserver/remote-printing/temp/
LOG_FILE=/home/printserver/remote-printing/logs/cron.log
PRINT_LOG_FILE=/home/printserver/remote-printing/logs/print.log

# Python Configuration
PYTHON_SCRIPT=/home/printserver/remote-printing/print_job.py
PYTHON_BIN=/usr/bin/python3

# Printer Configuration
DEFAULT_PRINTER=HP_LaserJet

# Cleanup Settings
DELETE_AFTER_PRINT=true
KEEP_LOGS_DAYS=30
